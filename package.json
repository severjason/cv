{
  "name": "cv",
  "version": "1.0.3",
  "author": "Sergii Severynov",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "analyze": "ANALYZE=true yarn build",
    "start": "next start",
    "type-check": "tsc --pretty --noEmit",
    "format": "prettier --write '**/*.{js,ts,tsx}' -u",
    "lint": "eslint . --ext ts --ext tsx --ext js",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.@(ts|tsx)": [
      "npm run lint",
      "npm run format"
    ]
  },
  "babel": {
    "presets": [
      "next/babel"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.10.6",
    "@emotion/server": "^11.10.0",
    "@emotion/styled": "^11.10.6",
    "@mui/icons-material": "^5.11.16",
    "@mui/material": "^5.12.1",
    "@next/bundle-analyzer": "^13.3.1",
    "@types/react-helmet": "^6.1.6",
    "cookies-next": "^2.1.1",
    "dayjs": "^1.11.7",
    "eslint-config-next": "^13.3.1",
    "i18next": "^22.4.15",
    "mongoose": "^7.0.5",
    "next": "^13.3.1",
    "next-connect": "^1.0.0-next.3",
    "next-i18next": "^13.2.2",
    "next-pwa": "^5.5.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-i18next": "^12.1.0",
    "sharp": "^0.32.0"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^13.3.1",
    "@trivago/prettier-plugin-sort-imports": "^4.1.1",
